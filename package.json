{
    "name": "go-extension",
    "displayName": "GO VScode Extension",
    "description": "GO VSCode plugin as helper for developers",
    "publisher": "Soerja",
    "repository": "https://github.com/soerjadi/go-extension",
    "license": "SEE LICENSE IN LICENSE",
    "version": "0.0.5",
    "engines": {
        "vscode": "^1.36.0",
        "node": ">=6.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:go"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.repository",
                "title": "GO: Generate repository"
            },
            {
                "command": "extension.model",
                "title": "Go: Generate model"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.36.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1",
        "vscode-test": "^1.0.2"
    },
    "dependencies": {
        "pascal-case": "^2.0.1",
        "snake-case": "^2.1.0"
    }
}
