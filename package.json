{
    "name": "go-extension",
    "displayName": "GO VScode Extension",
    "description": "GO VSCode plugin as helper for developers",
    "publisher": "Soerja",
    "repository": "https://github.com/soerjadi/go-extension",
    "license": "SEE LICENSE IN LICENSE",
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.36.0",
        "node": ">=6.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:go"
    ],
    "icon": "assets/go-extension.png",
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.repository",
                "title": "Go: Generate Repository"
            },
            {
                "command": "extension.usecase",
                "title": "Go: Generate Usecase"
            }
        ],
        "snippets": [
            {
                "language": "go",
                "path": "./go-snippet.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.36.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "tslint": "^5.12.1",
        "typescript": "^3.9.7",
        "vscode-test": "^1.0.2"
    },
    "dependencies": {
        "-": "^0.0.1",
        "camel-case": "^3.0.0",
        "pascal-case": "^2.0.1",
        "snake-case": "^2.1.0",
        "vsce": "^1.83.0"
    }
}
